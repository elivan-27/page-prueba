
var hashtable=new Array();function afterFirstInput(elementId,formName){blockElement1=document.getElementById(formName+':'+elementId+'_block1');blockElement2=document.getElementById(formName+':'+elementId+'_block2');inputElement1=document.getElementById(formName+':'+elementId);inputElement2=document.getElementById(formName+':'+elementId+'_input2');msgErrorElement=document.getElementById(formName+':'+elementId+'_msg_error');msgDigiteElement1=document.getElementById(formName+':'+elementId+'_msg_digite1');msgDigiteElement2=document.getElementById(formName+':'+elementId+'_msg_digite2');msgCofirmacionElement=document.getElementById(formName+':'+elementId+'_msg_confirmado');inputElement1.value=trim(inputElement1.value);hashtable[formName+':'+elementId]=inputElement1.value;if(hashtable[formName+':'+elementId]!=''&&hashtable[formName+':'+elementId]!=inputElement2.value){blockElement2.style.width=blockElement1.offsetWidth+'px';msgDigiteElement1.style.display='none';msgDigiteElement2.style.display='block';inputElement1.value='';inputElement2.value='';inputElement2.focus();}}
function afterSecondInput(elementId,formName){blockElement1=document.getElementById(formName+':'+elementId+'_block1');blockElement2=document.getElementById(formName+':'+elementId+'_block2');inputElement1=document.getElementById(formName+':'+elementId);inputElement2=document.getElementById(formName+':'+elementId+'_input2');msgErrorElement=document.getElementById(formName+':'+elementId+'_msg_error');msgDigiteElement1=document.getElementById(formName+':'+elementId+'_msg_digite1');msgDigiteElement2=document.getElementById(formName+':'+elementId+'_msg_digite2');msgCofirmacionElement=document.getElementById(formName+':'+elementId+'_msg_confirmado');inputElement2.value=trim(inputElement2.value);if(hashtable[formName+':'+elementId]==''||inputElement2.value==''||hashtable[formName+':'+elementId]!=inputElement2.value){inputElement1.value='';hashtable[formName+':'+elementId]='';inputElement2.value='';blockElement2.style.width='0px';msgDigiteElement1.style.display='block';msgDigiteElement2.style.display='none';inputElement1.style.backgroundColor='#ffcccc';inputElement1.focus();}else{msgDigiteElement1.style.display='block';msgDigiteElement2.style.display='none';blockElement2.style.width='0px';inputElement1.style.backgroundColor='#ccffcc';inputElement1.value=hashtable[formName+':'+elementId];}}
function contentChange(elementId,formName){inputElement1=document.getElementById(formName+':'+elementId);inputElement2=document.getElementById(formName+':'+elementId+'_input2');var pressedKeyCode=event.keyCode;var pressedKey=String.fromCharCode(pressedKeyCode).toLowerCase();if(pressedKeyCode==13){inputElement1.value='';}
if(event.ctrlKey&&(pressedKey=="c"||pressedKey=="v")){event.returnValue=false;}
if(inputElement1.value!=inputElement2.value){hashtable[formName+':'+elementId]
inputElement1.style.backgroundColor="#ffffff";hashtable[formName+':'+elementId]='';}}
function disableControlV(elementId,formName){var pressedKeyCode=event.keyCode;var pressedKey=String.fromCharCode(pressedKeyCode).toLowerCase();if(pressedKeyCode==13){afterSecondInput(elementId,formName);}
if(event.ctrlKey&&(pressedKey=="c"||pressedKey=="v")){event.returnValue=false;}}
function trim(x){return x.replace(/^\s+|\s+$/g,'');}